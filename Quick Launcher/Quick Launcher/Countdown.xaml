<UserControl x:Class="Quick_Launcher.Countdown"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Quick_Launcher"
             mc:Ignorable="d" 
             d:DesignHeight="240" d:DesignWidth="480">
    <Grid Background="#7FFFFFFF">
        <Label x:Name="lbZK" Content="距离中考还有" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0" FontSize="24"/>
        <Label x:Name="lbCountDownZK" Content="Loading.." HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" FontSize="36"/>
        <Label x:Name="lbYM" Content="距离一模还有" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,110,0,0" FontSize="24"/>
        <Label x:Name="lbCountDownYM" Content="Loading.." HorizontalAlignment="Left" Margin="10,157,0,0" VerticalAlignment="Top" FontSize="36"/>
    </Grid>
</UserControl>
