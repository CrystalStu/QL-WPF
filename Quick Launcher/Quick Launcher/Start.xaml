        <Label Content="Quick Launcher" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" FontSize="72"/>
        <Label x:Name="lb_stat" Content="" HorizontalAlignment="Left" VerticalAlignment="Bottom" Foreground="White" FontSize="18" Width="800" Background="#FF1E1E1E"/>
        <MediaElement Source="src\loading.gif" Height="100" Margin="690,161,10,39" Width="100" Visibility="Hidden"/>
    </Grid>
</Window>
