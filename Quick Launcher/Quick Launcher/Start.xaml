<Window x:Class="Quick_Launcher.Start"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quick_Launcher"
        mc:Ignorable="d"
        Title="Quick Launcher - Starting" Height="300" Width="800" ResizeMode="NoResize" WindowStyle="None" Background="Black" WindowStartupLocation="CenterScreen" Icon="src/ico.ico" MouseDown="Window_MouseDown">
